# Ignore node_modules for both frontend and backend
/backend/node_modules/
/frontend/node_modules/

# Ignore environment files
/backend/.env

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build outputs (if applicable)
frontend/build/
backend/dist/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore IDE/editor configs
.vscode/
.idea/

